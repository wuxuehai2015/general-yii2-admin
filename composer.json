    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "mdmsoft/yii2-admin": "^2.0@dev",
        "dmstr/yii2-adminlte-asset": "^2.0@dev",
        "kucha/ueditor": "*",
        "yiisoft/yii2-queue": "^2.0@dev",
        "kartik-v/yii2-dialog": "*",
        "yiisoft/yii2-redis": "^2.0@dev",
        "xiaosier/libweibo": "dev-master",
        "overtrue/easy-sms": "dev-master",
        "kartik-v/yii2-export": "@dev",
        "himiklab/yii2-sitemap-module": "*",
        "overtrue/wechat": "~4.0",
        "yansongda/pay": "dev-master",
        "moonlandsoft/yii2-phpexcel": "dev-master",
        "kriss/yii2-wang-editor": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": true
